<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <servlet>
        <servlet-name>HtmlOrderServlet</servlet-name>
        <servlet-class>org.archfirst.jsonoms.HtmlOrderServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HtmlOrderServlet</servlet-name>
        <url-pattern>/placeHtmlOrder</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AjaxOrderServlet</servlet-name>
        <servlet-class>org.archfirst.jsonoms.AjaxOrderServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AjaxOrderServlet</servlet-name>
        <url-pattern>/placeAjaxOrder</url-pattern>
    </servlet-mapping>

</web-app>